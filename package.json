    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "ts-standard": "^11.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
