    "@mui/material": "^5.13.7",
    "@react-oauth/google": "^0.11.0",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "axios": "^1.4.0",
    "moment": "^2.29.4",
    "msw": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.14.1",
    "vite-plugin-environment": "^1.1.3"
